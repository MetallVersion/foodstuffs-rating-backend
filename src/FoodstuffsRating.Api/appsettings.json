{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "FoodstuffsRating": "Trace"
      }
    }
  },
  "AllowedHosts": "*",

  "Authentication:Own": {
    "Jwt": {
      "ExpirationInMinutes": 30
    },
    "RefreshToken": {
      "ExpirationInMinutes": 1440
    }
  },
  "Registration": {
    "PasswordOptions": {
      "RequiredLength": 8
    }
  }
}
